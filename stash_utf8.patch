+        if (totalVariance > 0) {
+            alpha = (nItems / (nItems - 1)) * (1 - (sumItemVariances / totalVariance));
+        }
+
+        return {
+            items: nItems,
+            alpha: alpha,
+            interpretation: getInterpretation(alpha)
+        };
